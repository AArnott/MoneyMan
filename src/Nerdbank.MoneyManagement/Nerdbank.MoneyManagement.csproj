<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard1.6;net46</TargetFrameworks>
    <DocumentationFile>bin\$(Configuration)\Nerdbank.MoneyManagement.xml</DocumentationFile>
    <CodeAnalysisRuleSet>MoneyMan.ruleset</CodeAnalysisRuleSet>
    <NoWarn>1701;1702;1705;CS1591</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="StyleCop.json" />
  </ItemGroup>
  <ItemGroup>
    <AdditionalFiles Include="stylecop.json" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="PCLCommandBase" Version="0.3.2" />
    <PackageReference Include="sqlite-net-pcl" Version="1.3.1" />
    <PackageReference Include="Stylecop.Analyzers" Version="1.0.0" />
    <PackageReference Include="System.Collections.Immutable" Version="1.3.1" />
    <PackageReference Include="Validation" Version="2.3.7" />
  </ItemGroup>
</Project>